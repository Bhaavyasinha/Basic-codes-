#include <stdio.h>
int main() {
    int n;
    int m;
    printf ("Enter the row of the pattern\n");
    scanf ("%d",&n);
     printf ("Enter the column of the pattern\n");
    scanf ("%d",&m);
    for (int i = 0; i < n; i++){
        for (int j = 1; j <= m; j++){
            printf ("%d ",j);
        }
         printf ("\n");
    }
    return 0;
}
