#include <stdio.h>

int main() {
    int n;
    int m;
    printf ("Enter the lenght of the pattern\n");
    scanf ("%d",&n);
     printf ("Enter the breadth of the pattern\n");
    scanf ("%d",&m);
    for (int i = 0; i < m; i++){
        for (int j = 0; j < n; j++){
            printf ("* ");
        }
         printf ("\n");
    }
    return 0;
}
